# Azure Blob Storage
blob_connection_string=DefaultEndpointsProtocol=https;AccountName=<your_account_name>;AccountKey=<your_account_key>;EndpointSuffix=core.windows.net
container_name=<your_container_name>
parsed_container_name=<your_parsed_container_name>
embeddings_container_name=<your_embeddings_container_name>

# Azure Document Intelligence (Step 2)
endpoint=https://<your_document_intelligence_endpoint>.cognitiveservices.azure.com/
key=<your_document_intelligence_key>

# Azure OpenAI (Step 3)
openai_endpoint=https://<your_openai_endpoint>.openai.azure.com/openai/deployments/<your_deployment_name>/embeddings?api-version=2023-05-15
openai_key=<your_openai_key>
deployment_name=<your_deployment_name>

# Azure AI Search (Step 4)
search_endpoint=https://<your_search_endpoint>.search.windows.net
search_key=<your_search_key>
index_name=<your_index_name>